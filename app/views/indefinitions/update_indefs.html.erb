<% data = @update_indefs %>
<% for record in data %>
	<% if Indefinition.exists?(name: "#{record['name']}") %>
	<% else %>
	<% 	Indefinition.create!(name: "#{record['name']}", description: "#{record['description']}") %>
	<% end %>
<% end %>


